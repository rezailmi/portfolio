{
  "name": "direct-edit",
  "version": "0.1.0",
  "description": "Figma-style visual CSS editor for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "css",
    "visual-editor",
    "tailwind",
    "figma",
    "devtools"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-alpha.10",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.24.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  }
